I"Ñ(function() {
  $('#loginButton').click(function(e) {
    var user;
    e.preventDefault();
    user = {};
    user.email = $('#iEmail').val();
    user.password = $('#iPass').val();
    return $.ajax('api/v1/auth/login', {
      type: 'post',
      dataType: 'json',
      data: user,
      success: function(data) {
        return console.log(data);
      },
      error: function(error) {
        return console.log(error);
      }
    });
  });

}).call(this);
