I"“(function() {
  $('#loginButton').click(function(e) {
    var user;
    e.preventDefault();
    user = {
      auth: {}
    };
    user.auth.email = $('#iEmail').val();
    user.auth.password = $('#iPass').val();
    if (user.auth.email === '' || user.auth.password === '') {
      swal('Falta informaci√≥n', 'Por favor rellene todos los campos.', 'error');
      return;
    }
    return $.ajax('api/v1/auth/login', {
      type: 'post',
      dataType: 'json',
      data: user,
      success: function(response) {
        if (response.user.user_type.name === 'Administrator') {
          localStorage.setItem('token', response.auth_token);
          delete response.auth_token;
          localStorage.setItem('user', response);
          return window.location.href = '/dashboard';
        }
      },
      error: function(error) {
        return swal('Credenciales invalidas', 'El usuario y/o la contrase√±a son incorrectos', 'error');
      }
    });
  });

}).call(this);
